<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myschool">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/education"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/security_config"
        android:roundIcon="@drawable/education"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AdviceActivity"></activity>
        <activity android:name=".LogsActivity" />
        <activity android:name=".QuizzRes" />
        <activity android:name=".CurrentQuizz" />
        <activity android:name=".Quizz" />
        <activity android:name=".CurrentCourse" />
        <activity android:name=".MyCourses" />
        <activity android:name=".ParentDashboardActivity" />
        <activity android:name=".DashboardActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".AuthActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>